{
  "name": "trezor.js-node",
  "version": "5.8.0",
  "author": "TREZOR <info@trezor.io>",
  "homepage": "https://github.com/trezor/trezor.js",
  "description": "High-level Javascript API for Bitcoin Trezor for node",
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/trezor/trezor.js.git"
  },
  "bugs": {
    "url": "https://github.com/trezor/trezor.js/issues"
  },
  "keywords": [
    "trezor",
    "bitcoin",
    "wallet"
  ],
  "main": "lib/index-node.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-flow-strip-types",
      "transform-class-properties",
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.18.0",
    "bump-version": "^0.5.0"
  },
  "dependencies": {
    "bitcoinjs-lib-zcash": "^2.3.0",
    "ecurve": "^1.0.2",
    "node-fetch": "^1.6.0",
    "randombytes": "^2.0.1",
    "semver-compare": "1.0.0",
    "trezor-link-node": "0.2.98",
    "unorm": "^1.3.3"
